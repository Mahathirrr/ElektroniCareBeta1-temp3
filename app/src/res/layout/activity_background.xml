<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:id="@+id/rnm4dncivm6i"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:gravity="center_horizontal"
		android:background="#7C3AED"
		android:paddingHorizontal="83dp">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<ImageView
				android:id="@+id/r8u2h6ux9wvl"
				android:layout_width="104dp"
				android:layout_height="133dp"
				android:layout_marginTop="300dp"
				android:src="@mipmap/ic_launcher"
				android:scaleType="fitXY"
			/>
			<TextView
				android:id="@+id/rofmjdvd2tc"
				android:textColor="#FFFFFF"
				android:textSize="33sp"
				android:textStyle="bold"
				android:layout_marginBottom="8dp"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:text="ElektroniCare"
			/>
			<TextView
				android:id="@+id/re8q1kls9l59"
				android:textColor="#EDE9FE"
				android:textSize="16sp"
				android:layout_marginBottom="300dp"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:text="Expert Electronics Repair"
			/>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
